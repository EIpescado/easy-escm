<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.group1418.easy.escm.core.system.mapper.SystemClientMapper">
    <select id="list" resultType="org.group1418.easy.escm.core.system.pojo.to.SystemClientTo">
     select  * from system_client where enabled = 1
    </select>

    <select id="get" resultType="org.group1418.easy.escm.core.system.pojo.vo.SystemClientVo">
        select * from system_client where id = #{id} and enabled = 1
    </select>
    <select id="getByClientId" resultType="org.group1418.easy.escm.core.system.pojo.vo.SystemClientVo">
        select * from system_client where client_id = #{clientId} and enabled = 1
    </select>

</mapper>
